git init

git config 
git config --list
git config --lidt --show-origin
git config --global user.name "<name>"
git config --global user.email "<email>"

git status 

git add <file> 
git add .
git commit -m "<menssage>"
git commit -am "<menssage>"

git log 
git log <file>
git log --stat
git log --all
git log --all --graph --decorate --oneline
git show <file>
git diff <id_commit1> <id_commit2>

git rm <file>
git rm --cached <file>
git reset <id commit> --hard
git reset <id_commit> --soft
git reset HEAD

git branch
git branch <name>
git checkout <branch>
git merge <branch>

git checkout <id_commit> <file>
git checkout <branch> <file>

git tag -a v0.1 -m <"mensaje"> <id_commit>
git tag
git show-ref --tags

GITHUB

git remote add origin <URL>
git remote
git remote -v

git clone <url>

git push <url>

git fetch + git merge

git pull
git pull origin master --allow-unrelated-histories

ssh-keygen -t rsa -b 4096 -C "kamana108@hotmail.com"
eval $(ssh-agent -s) 
